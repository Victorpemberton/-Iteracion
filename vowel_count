names = ["victor", "peter", "Chris", "Alexandra", "Benjamin", "Victoria", "Olivia", "Sopha"]


def count_vowels(name):
    vowels = "aeiouAEIOU"
    return sum(1 for char in name if char in vowels)


for name in names:
    vowel_count = count_vowels(name)
    if vowel_count >= 3:
        print(f"{name}: {vowel_count} vowels")
        
